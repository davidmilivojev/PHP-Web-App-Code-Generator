
WebApp Konferencije[

  class +db konferencije_ias{

  entity Konferencija !m {
    $key (idKonferencije)idKonferencija =
    ^ naziv:input =
    opis:desc =
    rang <idRang>
    #links[
      +nazivRang*
    ]
    #paths[
     %c  Rang
     %d  RangDAL
    ]
    #actions[
     @konferencije : views
     @konferencija : view
     @dodajkonferenciju : create
     @izbrisikonferenciju : delete
     @izmenikonferenciju : edit
     @konferencijepretraga : !PrikaziKonferencijePretraga
     +@rangovi : views
    ]
  }

  entity Rang {
    $key (idRang)idRang =
    ^ nazivRang:input =
    #paths[
      %c InterfaceFieldValidator
      %c inputFormaterCustom
      %d CommonDatabaseMethods
    ]
    #actions[
     @rangovi : views
     @rang : view
     @rangpretraga : !RangPretaraga
     @izbrisirang : delete
     @dodajRang : create
    ]
  }



  }



]
